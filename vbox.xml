<domain type='vbox'>
  <name>ReactOS</name>
  <uuid>0ab69219-58c2-1e5c-6beb-fb29db74b0ae</uuid>
  <memory>524288</memory>
  <vcpu>1</vcpu>
  <os>
    <type>hvm</type>
    <boot dev='cdrom'/>
  </os>
  <features>
  </features>
  <clock offset='localtime'/>
